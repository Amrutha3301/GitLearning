import { Component } from '@angular/core';
import { CommonModule } from '@angular/common';

@Component({
  selector: 'app-buy-plan',
  standalone: true,
  imports: [CommonModule],
  templateUrl: './buy-plan.html',
  styleUrls: ['./buy-plan.css']
})
export class BuyPlanComponent {

  // UI STATE CONTROLLER
  step: 'AGE' | 'PLANS' | 'DETAILS' | 'CONFIRM' = 'AGE';

  selectedPlan: any = null;

  plans = [
    {
      id: 1,
      name: 'Care Life Plan',
      description: 'Instant coverage with emergency services.',
      price: 3150
    },
    {
      id: 2,
      name: 'Good Health Plan',
      description: 'Comprehensive health insurance.',
      price: 2800
    }
  ];

  answerAge(isBelow30: boolean) {
    this.step = 'PLANS';
  }

  openDetails(plan: any) {
    this.selectedPlan = plan;
    this.step = 'DETAILS';
  }

  buyNow() {
    this.step = 'CONFIRM';
  }
}   take data from db.json


